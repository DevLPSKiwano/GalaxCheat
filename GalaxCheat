AcsAmmoButton.MouseButton1Click:Connect(function()
	scrollingFrame.Visible = false
	local screenGui = Instance.new("ScreenGui")
	screenGui.Name = "Proute"
	screenGui.Parent = game.Players.LocalPlayer.PlayerGui

	local textBox = Instance.new("TextBox")
	textBox.Name = "Neuille"
	textBox.Size = UDim2.new(0, 250, 0, 50)
	textBox.Position = UDim2.new(0.5, -125, 0.4, -25)
	textBox.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
	textBox.BorderColor3 = Color3.fromRGB(100, 100, 100)
	textBox.TextColor3 = Color3.fromRGB(255, 255, 255)
	textBox.Font = Enum.Font.GothamBold
	textBox.TextSize = 20
	textBox.PlaceholderText = "Nom Arme:"
	textBox.TextWrapped = true
	textBox.ClearTextOnFocus = true
	textBox.Parent = screenGui

	local button = Instance.new("TextButton")
	button.Name = "Siuuuuu"
	button.Size = UDim2.new(0, 120, 0, 50)
	button.Position = UDim2.new(0.5, -60, 0.55, -25)
	button.BackgroundColor3 = Color3.fromRGB(50, 150, 50)
	button.BorderColor3 = Color3.fromRGB(0, 100, 0)
	button.TextColor3 = Color3.fromRGB(255, 255, 255)
	button.Font = Enum.Font.GothamBold
	button.TextSize = 22
	button.Text = "Executer"
	button.Parent = screenGui

	local function applyValues()
		local toolName = textBox.Text
		local player = game.Players.LocalPlayer
		local backpack = player.Backpack

		local tool = backpack:FindFirstChild(toolName)
		if tool then
			local ammo = tool:FindFirstChild("ACS_Modulo") and tool.ACS_Modulo:FindFirstChild("Variaveis") and tool.ACS_Modulo.Variaveis:FindFirstChild("Ammo")
			local storedAmmo = tool:FindFirstChild("ACS_Modulo") and tool.ACS_Modulo:FindFirstChild("Variaveis") and tool.ACS_Modulo.Variaveis:FindFirstChild("StoredAmmo")

			if ammo and storedAmmo then
				storedAmmo.Value = 99999
				storedAmmo.MaxValue = 99999
				ammo.Value = 99999
				screenGui.Enabled = false
			else
				print("CACA")
			end
		else
			print("CACA")
		end
	end

	button.MouseButton1Click:Connect(applyValues)

end)

InfinityYieldButton.MouseButton1Click:Connect(function()
	loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
	scrollingFrame.Visible = false
end)

TpTool.MouseButton1Click:Connect(function()
	local mouse = game.Players.LocalPlayer:GetMouse()
	local tool = Instance.new("Tool")
	tool.RequiresHandle = false
	tool.Name = "TP Tool"
	tool.Activated:connect(function()
		local pos = mouse.Hit+Vector3.new(0,2.5,0)
		pos = CFrame.new(pos.X,pos.Y,pos.Z)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
	end)
	tool.Parent = game.Players.LocalPlayer.Backpack
	scrollingFrame.Visible = false
	
end)

InvFling.MouseButton1Click:Connect(function()
	loadstring(game:HttpGet("https://pastebin.com/raw/WvUnM1LY"))()
end)

SpamButton.MouseButton1Click:Connect(function()
	while true do
		task.wait(3)
		print("Galax Owned By Lyks")
		print("Gacha la lyks t'encule ")
	end
end)

AcsSound.MouseButton1Click:Connect(function()
	scrollingFrame.Visible = false
	local screenGui = Instance.new("ScreenGui")
	screenGui.Name = "dfsdas"
	screenGui.Parent = game.Players.LocalPlayer.PlayerGui



	local button = Instance.new("TextButton")
	button.Name = "dsadas"
	button.Size = UDim2.new(0, 120, 0, 50)
	button.Position = UDim2.new(0.5, -60, 0.55, -25)
	button.BackgroundColor3 = Color3.fromRGB(50, 150, 50)
	button.BorderColor3 = Color3.fromRGB(0, 100, 0)
	button.TextColor3 = Color3.fromRGB(255, 255, 255)
	button.Font = Enum.Font.GothamBold
	button.TextSize = 22
	button.Text = "Jouer"
	button.Parent = screenGui

	local Sound =  game.ReplicatedStorage.ACS_Engine.FX.EarRing

	button.MouseButton1Click:Connect(function()
		screenGui.Enabled = false
		Sound.Playing = true
		Sound.Volume = 100
		Sound.MaxDistance = 2000
	end)
end)

DestroyPartTool.MouseButton1Click:Connect(function()
	local player = game.Players.LocalPlayer
	local mouse = player:GetMouse()
	local tool = Instance.new("Tool")
	tool.RequiresHandle = false
	tool.Name = "DestroyPart"
	mouse.Button1Down:Connect(function()
		local target = mouse.Target
		if target and target:IsA("Part") then
			target:Destroy()
		end
	end)
	tool.Parent = game.Players.LocalPlayer.Backpack
	scrollingFrame.Visible = false
end)

ColorPartTool.MouseButton1Click:Connect(function()
	local player = game.Players.LocalPlayer
	local mouse = player:GetMouse()
	local tool = Instance.new("Tool")
	tool.RequiresHandle = false
	tool.Name = "ColorPart"
	mouse.Button1Down:Connect(function()
		local target = mouse.Target
		if target and target:IsA("Part") then
			target.BrickColor = BrickColor.random()
  		end
	end)
	tool.Parent = game.Players.LocalPlayer.Backpack
	scrollingFrame.Visible = false
end)

ExploseMapButton.MouseButton1Click:Connect(function()
	ExploseMapButton.MouseButton1Click:Connect(function()
		for _, part in pairs(workspace:GetDescendants()) do
			if part:IsA("Part") then
				local explosion = Instance.new("Explosion")
				explosion.Position = part.Position
				explosion.Parent = workspace
				explosion.BlastRadius = 10
				explosion.BlastPressure = 5000 
				explosion.ExplosionType = Enum.ExplosionType.Craters 
			end
		end
	end)

end)



OwlAimbot.MouseButton1Click:Connect(function()
	scrollingFrame.Visible = false
	loadstring(game:HttpGet("https://raw.githubusercontent.com/CriShoux/OwlHub/master/OwlHub.txt"))();
end)


AcsStamina.MouseButton1Click:Connect(function()
	scrollingFrame.Visible = false
	local StarterCharacterScript = game.StarterPlayer.StarterCharacterScripts

	local Stamina = StarterCharacterScript.ACS_Client.Variaveis.Stamina
	Stamina.MaxValue = 8999999
	Stamina.MaxValue = 9000000
	Stamina.Value = 9000000
end)

GiveToolButton.MouseButton1Click:Connect(function()
	scrollingFrame.Visible = false
	for i,v in ipairs(game.ReplicatedStorage:GetDescendants()) do
		if v:IsA("Tool") then
			v:Clone().Parent = game.Players.LocalPlayer.Backpack
		end
		
	end
end)

WalkspeedButton.MouseButton1Click:Connect(function()
	scrollingFrame.Visible = false
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 70

end)

JumpPowerButton.MouseButton1Click:Connect(function()
	scrollingFrame.Visible = false
	game.Players.LocalPlayer.Character.Humanoid.JumpHeight = 70
end)
